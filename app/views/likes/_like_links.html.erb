  <% if current_user.likes.find_by(note_id: note.id) %>
    <%= link_to fa_icon("heart"), unlike_path(note.id), method: :delete, remote: true,:style=>"color:red;" %>
  <% else %>
    <%= link_to fa_icon("heart-o"), like_path(note.id), method: :post, remote: true,:style=>"color:black;" %>
  <% end %>